class Solution:
    def subarraySum(self, arr, target):
        total = 0
        dic = {0:0}
        
        for i in range(len(arr)):
            total += arr[i]
            
            if total-target in dic:
                if total - target == 0:
                   return [1, i+1]
                else:
                    return [dic[total-target]+1, i+1]
            elif total-target not in dic:
                dic[total] = i+1    

        return [-1]



