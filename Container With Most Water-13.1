class Solution:
    def maxWater(self, arr):
        n = len(arr)
        l, r = 0, n-1
        ma = 0
        
        while l<r:
            h = min(arr[l], arr[r])
            w = r-l
            ma = max(ma, h*w)
            
            if arr[l] < arr[r]:
                l += 1
            else:
                r -= 1
        
        return ma




