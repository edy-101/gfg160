class Solution:
    def longestSubarray(self, arr, k):  
        # code here
        ps = 0
        ml = 0
        sm = {}
        
        for i, n in enumerate(arr):
            ps += n
            if ps == k:
                ml = i+1
            if (ps - k) in sm:
                ml = max(ml, i - sm[ps-k])
            if ps not in sm:
                sm[ps] = i

        return ml




