class Solution:
    def subarrayXor(self, arr, k):
        # code here
        freq = {}
        xor = 0
        count = 0
        
        for num in arr:
            xor ^= num
            if xor == k:
                count += 1
            if xor ^ k in freq:
                count += freq[xor ^ k]
            freq[xor] = freq.get(xor, 0) + 1
        
        return count




